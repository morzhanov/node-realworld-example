    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-toastify": "^5.0.0-rc.3",
    "uuidv4": "^3.0.1"
  },

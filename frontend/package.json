{
  "name": "node-realworld-example-frontend",
  "version": "1.0.0",
  "description": "node-realworld-example frontend app",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server  --config config/webpack.config.dev.js --mode development --port 4000",
    "bld": "sh -ac '. ./.env.${REACT_APP_BUILD_MODE}; node ./config/scripts/build.js'",
    "build": "NODE_ENV=development REACT_APP_BUILD_MODE=development yarn run bld",
    "staging": "NODE_ENV=production REACT_APP_BUILD_MODE=staging yarn run bld",
    "production": "NODE_ENV=production REACT_APP_BUILD_MODE=production yarn run bld && yarn run clean:production && yarn run copy:production",
    "clean:production": "rm -rf ../src/public",
    "copy:production": "cp -a ./build ../src/public",
    "test": "jest -t ''",
    "test:update": "jest --updateSnapshot",
    "test:extended": "node scripts/test.js --env=jsdom",
    "lint": "eslint src --ext .js --ext .jsx"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
